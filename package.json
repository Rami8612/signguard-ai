{
  "name": "signguard-ai",
  "version": "0.9.0",
  "description": "Decode Ethereum calldata into human-understandable consequences",
  "type": "module",
  "bin": {
    "decode": "./bin/decode.js"
  },
  "scripts": {
    "start": "node bin/decode.js",
    "api": "node api/server.js",
    "dev": "concurrently \"npm run api\" \"npm run web:dev\"",
    "web:dev": "cd web && npm run dev"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "ethers": "^6.11.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ethereum",
    "calldata",
    "decoder",
    "transaction",
    "security"
  ],
  "license": "MIT"
}
